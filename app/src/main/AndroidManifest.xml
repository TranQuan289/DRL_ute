<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.drl">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ute"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ute"
        android:supportsRtl="true"
        android:theme="@style/Theme.DRL">
        <activity
            android:name=".Start_ui.ResetPassActivity"
            android:exported="true" />
        <activity
            android:name=".Teacher_ui.MainTeacherActivity"
            android:exported="true" />
        <activity
            android:name=".Start_ui.SignInActivity"
            android:exported="true" />
        <activity
            android:name=".Student_ui.MainStudentActivity"
            android:exported="true" />
        <activity
            android:name=".Start_ui.SplashActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.DRL.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>